<?xml version="1.0" encoding="utf-8"?>
<Konfiguracija>
    <!-- Minimalne godine za registraciju korisnika -->
    <MinimalneGodine>12</MinimalneGodine>

    <!-- Ograničenja trajanja boravka u terminima (minimalni i maksimalni broj termina po korisniku) -->
    <MinTerminiPoKorisniku>1</MinTerminiPoKorisniku>
    <MaxTerminiPoKorisniku>5</MaxTerminiPoKorisniku>

    <!-- Ograničenje broja termina po treneru dnevno -->
    <MaxTerminiPoTreneru>8</MaxTerminiPoTreneru>
</Konfiguracija>
